function isprime( num){
  flag = true;
  if(num<2)
    flag=false;
  for(var i = 2; i <=Math.sqrt(num); i++){
   if(num % i == 0) {
     flag = false;
     break;
   }
  }
 return flag;
  
  }

function printprime(lb, ub){
  for(var i = lb; i <= ub; i++){
    if(isprime(i))
      console.log(i);
  }
}
printprime(5, 100);
